<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xml" href="{{ site.url }}{{ site.baseurl }}/feed.xslt.xml"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <link href="{{ site.url }}{{ site.baseurl }}/feeds/a11y.xml" rel="self" type="application/atom+xml" />
  <link href="{{ site.url }}{{ site.baseurl }}" rel="alternate" type="text/html" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://localhost:4000/blog/a11y/</id>
  <category term="daj-sie-poznac-2017" />
  <title type="html">Comandeerâ€™s blog</title>
  <subtitle>Kategoria 'a11y'</subtitle>

  {% for post in site.categories.a11y limit:10 %}
  <entry>
    <title type="html">{{ post.title | xml_escape }}</title>
    {% if post.author %}
    <author>
      <name>{{ post.author | xml_escape }}</name>
    </author>
    {% endif %}
    <link href="{{ site.url }}{{ site.baseurl }}{{ post.url }}" rel="alternate" type="text/html" />
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>{{ site.url }}{{ site.baseurl }}{{ post.url }}</id>
    <content type="html" xml:base="{{ site.url }}{{ site.baseurl }}{{ post.url }}">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
